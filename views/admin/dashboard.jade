extends ../layout

block content
  img.welcome__home-image(src= base +'/images/homeicon.svg')
  .welcome__home-message.ng-binding Welcome to ChemoFinder
  .welcome__home-buttons
    table#myTable.display
      thead
        tr
          th Column 1
          th Column 2
      tbody
        tr
          td Row 1 Data 1
          td Row 1 Data 2
        tr
          td Row 2 Data 1
          td Row 2 Data 2
block scripts
  script(type='text/javascript', src='https://cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js')
  script(type='text/javascript').
    $(document).ready(function(){
    $('#myTable').DataTable();
    });

